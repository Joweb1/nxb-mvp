{
  "expo": {
    "name": "nxb-mvp",
    "slug": "nxb-mvp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nxb-logo.png",
    "scheme": "nxbmvp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/nxb-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nxbmvp.app"
    },
    "android": {
      "package": "com.nxbmvp.app",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/nxb-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/nxb-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nxb-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f04f555-36cd-4be9-aa56-0667437719d9"
      }
    },
    "owner": "codewithmobile"
  }
}
